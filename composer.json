{
    "name": "ybelenko/openapi-data-mocker",
    "description": "Library that generates fake data from Swagger 2.0|Openapi 3.0 spec",
    "type": "library",
    "license": "MIT",
    "keywords": ["openapi", "swagger", "data", "mocker", "faker", "mock", "fake"],
    "authors": [
        {
            "name": "Yuriy Belenko",
            "email": "yura-bely@mail.ru"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.2",
        "ybelenko/openapi-data-mocker-interfaces": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "OpenAPIServer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OpenAPIServer\\": "test/"
        }
    },
    "scripts": {
        "test": [
            "@phpunit",
            "@phpcs"
        ],
        "phpunit": "phpunit --coverage-text",
        "phpcs": "phpcs"
    },
    "repositories": [
        {
            "url": "https://github.com/ybelenko/openapi-data-mocker-interfaces.git",
            "type": "github",
            "no-api": true
        }
    ]
}
